@model MemberUpdateDto
@{
    SelectList categories = ViewData["Plans"] as SelectList;
}

<form class="container" method="POST" enctype="multipart/form-data">
    <h2 class="my-4">Update </h2>
    <div class="mb-3">
        <label class="form-label" asp-for="Name"></label>
        <input class="form-control" asp-for="Name" />
        <span class="text-danger" asp-validation-for="Name"></span>
    </div>
    <div class="mb-3">
        <label class="form-label" asp-for="Position"></label>
        <input class="form-control" asp-for="Position" />
        <span class="text-danger" asp-validation-for="Position"></span>
    </div>
    <div class="mb-3">
        <label class="form-label" asp-for="Image"></label>
        <input class="form-control" asp-for="Image" />
        <span class="text-danger" asp-validation-for="Image"></span>
    </div>
 
    <div class="mb-3">
        <label class="form-label" asp-for="Surname"></label>
        <input class="form-control" asp-for="Surname" />
        <span class="text-danger" asp-validation-for="Surname"></span>
    </div>
    <div class="mb-3">
        <label class="form-label" asp-for="Position"></label>
        <input class="form-control" asp-for="Position" />
        <span class="text-danger" asp-validation-for="Position"></span>
    </div>

   
    <p class="text-danger text-center">@Html.ValidationMessage("CustomError")</p>
    <button type="submit" class="btn btn-primary">Save Changes</button>
</form>